{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", "storybook-static/**"],
      "dependsOn": ["^build"]
    },
    "build-storybook": {
      "inputs": [".storybook/**", "src/**"],
      "outputs": ["storybook-static"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "lint:check": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "test:cov": {},
    "test:cov-total": {},
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
