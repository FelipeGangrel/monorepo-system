{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "Preserve",
    "target": "ES2022",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "moduleResolution": "Bundler",
    "types": ["node"],
    "skipLibCheck": true,
    "importHelpers": true
  }
}
