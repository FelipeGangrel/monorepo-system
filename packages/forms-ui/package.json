    "directory": "packages/forms-ui"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "types": "dist/index.d.ts"
